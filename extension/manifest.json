{
  "name": "Fast New Tab",
  "short_name": "FNT!",
  "version": "1.2.5",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "./src/newtab.html"
  },
  "minimum_chrome_version": "16",
  "description": "Fast new tab without deep scripts and requests.",
  "icons": {
    "128": "./icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16" : "./icons/icon128.png",
      "48" : "./icons/icon128.png",
      "128": "./icons/icon128.png"
    },
    "default_title": "Fast New Tab"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "homepage_url": "https://github.com/uid-11222/fast-new-tab",
  "offline_enabled": true,
  "background": {
    "scripts": ["./src/newtab.js"],
    "persistent": false
  }
}
