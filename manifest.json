{
  "name": "Fast New Tab",
  "short_name": "FNT!",
  "version": "0.7.1",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "./src/newtab.html"
  },
  "minimum_chrome_version": "16",
  "description": "Fast new tab without deep scripts and requests.",
  "icons": {
    "128": "./icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "./icons/icon128.png"
    },
    "default_title": "Fast New Tab"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en"
}
